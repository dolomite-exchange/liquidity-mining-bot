{
  "name": "liquidity-mining-bot",
  "version": "1.0.0",
  "description": "Microservice to bid on Dolomite liquidation auctions",
  "main": "build/src/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "coverage": "NODE_ENV=test jest --coverage --runInBand --forceExit --coverageReporters=text-lcov | node_modules/.bin/coveralls",
    "build": "rm -rf build/ && node_modules/.bin/tsc",
    "build:docker": "docker build -t dolomiteprotocol/liquidity-mining-bot:latest .",
    "start": "node --max-old-space-size=4096 build/src/index.js",
    "start:ts": "ts-node src/index.ts",
    "lint": "eslint . --ext .js --ignore-path .gitignore",
    "compose-clean": "docker-compose rm -f",
    "migrate": "echo 'Nothing to migrate'",
    "seed": "echo 'Nothing to seed'",
    "execute:arbitrum:calculate-assets-held-for-duration": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/calculate-assets-held-for-duration.ts",
    "execute:arbitrum:calculate-ez-eth-points": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/calculate-ez-eth-points.ts",
    "execute:arbitrum:calculate-mineral-rewards": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/calculate-mineral-rewards.ts",
    "execute:arbitrum:calculate-mineral-season-config": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/calculate-mineral-season-config.ts",
    "execute:arbitrum:calculate-otoken-rewards": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/calculate-otoken-rewards.ts",
    "execute:arbitrum:calculate-otoken-season-config": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/calculate-otoken-season-config.ts",
    "execute:arbitrum:get-global-otoken-data": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/get-global-otoken-vesting-data.ts",
    "execute:arbitrum:migrate-oarb-finalized-outputs": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/migrate-oarb-finalized-output.ts",
    "execute:arbitrum:rectify-oarb-rewards-for-arb": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/rectify-oarb-rewards-for-arb.ts",
    "execute:arbitrum:rectify-oarb-rewards-for-deltas": "SCRIPT=true ENV_FILENAME=detonator.arbitrum-one.production.env npx ts-node scripts/rectify-oarb-rewards-for-deltas.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolomite-exchange/liquidity-mining-bot.git"
  },
  "author": "Dolomite",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dolomite-exchange/liquidity-mining-bot/issues"
  },
  "homepage": "https://github.com/dolomite-exchange/liquidity-mining-bot#readme",
  "dependencies": {
    "@dolomite-exchange/dolomite-margin": "^0.11.4",
    "@dolomite-exchange/dolomite-margin-modules": "^0.0.13",
    "@dolomite-exchange/zap-sdk": "^0.2.44",
    "axios": "^1.1.3",
    "dotenv-flow": "^0.4.0",
    "ethers": "^5.5.3",
    "lru-cache": "^5.1.1",
    "luxon": "^1.28.1",
    "merkletreejs": "^0.3.11",
    "request-promise-native": "^1.0.9",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "coveralls": "^3.0.14",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "install": "^0.13.0",
    "jest": "^26.3.0",
    "jest-junit": "^10.0.0",
    "nodemon": "^1.19.4",
    "npm": "^8.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
