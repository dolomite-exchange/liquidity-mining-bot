{
  "name": "liquidity-mining-bot",
  "version": "1.0.0",
  "description": "Microservice to bid on Dolomite liquidation auctions",
  "main": "build/src/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "coverage": "NODE_ENV=test jest --coverage --runInBand --forceExit --coverageReporters=text-lcov | node_modules/.bin/coveralls",
    "build": "rm -rf build/ && node_modules/.bin/tsc",
    "build:docker": "docker build -t dolomiteprotocol/liquidity-mining-bot:latest .",
    "start": "node --max-old-space-size=4096 build/src/index.js",
    "start:ts": "ts-node src/index.ts",
    "lint": "eslint . --ext .js --ignore-path .gitignore",
    "compose-clean": "docker-compose rm -f",
    "migrate": "echo 'Nothing to migrate'",
    "seed": "echo 'Nothing to seed'",
    "execute:add-mineral-deltas": "SCRIPT=true npx ts-node scripts/add-mineral-deltas.ts",
    "execute:calculate-assets-held-for-duration": "SCRIPT=true npx ts-node scripts/calculate-assets-held-for-duration.ts",
    "execute:calculate-mineral-rewards": "SCRIPT=true npx ts-node scripts/calculate-mineral-rewards.ts",
    "execute:calculate-mineral-rewards-for-yt": "SCRIPT=true npx ts-node scripts/calculate-mineral-rewards-for-pendle.ts",
    "execute:calculate-mineral-season-config": "SCRIPT=true npx ts-node scripts/calculate-mineral-season-config.ts",
    "execute:calculate-options-airdrop": "SCRIPT=true npx ts-node scripts/calculate-options-airdrop.ts",
    "execute:calculate-otoken-rewards": "SCRIPT=true npx ts-node scripts/calculate-otoken-rewards.ts",
    "execute:calculate-otoken-season-config": "SCRIPT=true npx ts-node scripts/calculate-otoken-season-config.ts",
    "execute:calculate-regular-airdrop": "SCRIPT=true ENV_FILENAME=.env.production npx ts-node scripts/calculate-regular-airdrop.ts",
    "execute:get-all-berachain-testnet-users": "NETWORK_ID=80084 SCRIPT=true npx ts-node scripts/get-all-berachain-testnet-users.ts",
    "execute:get-queried-berachain-testnet-users": "NETWORK_ID=80084 SCRIPT=true npx ts-node scripts/get-queried-berachain-testnet-users.ts",
    "execute:get-all-events": "SCRIPT=true npx ts-node scripts/get-all-events.ts",
    "execute:get-all-prices": "SCRIPT=true npx ts-node scripts/get-all-prices.ts",
    "execute:get-all-smart-contract-users-from-airdrop": "SCRIPT=true npx ts-node scripts/get-all-smart-contract-users-from-airdrop.ts",
    "execute:get-all-virtual-events": "SCRIPT=true npx ts-node scripts/get-all-virtual-events.ts",
    "execute:get-global-otoken-data": "SCRIPT=true npx ts-node scripts/get-global-otoken-vesting-data.ts",
    "execute:migrate-oarb-finalized-outputs": "SCRIPT=true npx ts-node scripts/migrate-oarb-finalized-output.ts",
    "execute:pendle-balance-fetcher": "SCRIPT=true npx ts-node src/lib/pendle/main.ts",
    "execute:rectify-oarb-rewards-for-arb": "SCRIPT=true npx ts-node scripts/rectify-oarb-rewards-for-arb.ts",
    "execute:rectify-oarb-rewards-for-deltas": "SCRIPT=true npx ts-node scripts/rectify-oarb-rewards-for-deltas.ts",
    "execute:rectify-old-pendle-epochs": "SCRIPT=true npx ts-node scripts/rectify-old-pendle-epochs.ts",
    "execute:transform-csv-to-json": "SCRIPT=true npx ts-node scripts/transform-csv-to-json.ts",
    "execute:transform-all-pendle-minerals-to-events": "SCRIPT=true npx ts-node scripts/transform-all-pendle-minerals-to-events.ts",
    "execute:transform-regular-airdrop-to-json": "SCRIPT=true npx ts-node scripts/transform-regular-airdrop-to-json.ts",
    "execute:verify-all-prices": "SCRIPT=true npx ts-node scripts/verify-all-prices.ts",
    "flow:arb-stip:arbitrum": "ENV_FILENAME=local.arbitrum.production.env npx ts-node scripts/flow/execute-arb-stip-flow.ts",
    "flow:pendle-arb-stip:arbitrum": "ENV_FILENAME=local.arbitrum.production.env npx ts-node scripts/flow/execute-pendle-arb-stip-flow.ts",
    "flow:oarb-stip:arbitrum": "ENV_FILENAME=local.arbitrum.production.env npx ts-node scripts/flow/execute-oarb-stip-flow.ts",
    "flow:pendle-mineral:arbitrum": "ENV_FILENAME=local.arbitrum.production.env npx ts-node scripts/flow/execute-pendle-mineral-flow.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolomite-exchange/liquidity-mining-bot.git"
  },
  "author": "Dolomite",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dolomite-exchange/liquidity-mining-bot/issues"
  },
  "homepage": "https://github.com/dolomite-exchange/liquidity-mining-bot#readme",
  "dependencies": {
    "@dolomite-exchange/dolomite-margin": "^0.12.13",
    "@dolomite-exchange/zap-sdk": "^0.3.32",
    "csv-parser": "^3.1.0",
    "dotenv-flow": "^0.4.0",
    "ethers": "^5.5.3",
    "lru-cache": "^5.1.1",
    "luxon": "^1.28.1",
    "merkletreejs": "^0.3.11",
    "request-promise-native": "^1.0.9",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^16.11.68",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "coveralls": "^3.0.14",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "install": "^0.13.0",
    "jest": "^26.3.0",
    "jest-junit": "^10.0.0",
    "nodemon": "^1.19.4",
    "npm": "^8.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  }
}
